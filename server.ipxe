#!ipxe

# Location of Kernel/Initrd images
set base-url https://releases.rancher.com/os/latest

kernel ${base-url}/vmlinuz rancher.state.dev=LABEL=RANCHER_STATE -- rancher.cloud_init.datasources=[url:https://raw.githubusercontent.com/broberson/vultr-rancheros/master/cloud-config-server.yml]

initrd ${base-url}/initrd

boot

